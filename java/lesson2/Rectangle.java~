class Rectangle {
    int x1, y1, x2, y2;
    void setRectangle(double x1, double y1, double x2, double y2) {
	double a = Math.sqrt(Math.sqr(y2-y1));
	double b = Math.sqrt(Math.sqr(x2-x1));
    }
    double getArea() {
	return a * b;
    }
}

class RectangleDemo {
    public static void main(String[] args) {
	Rectangle rectangle = new Rectangle();
	rectangle.setRectangle(0, 0, 1, 1);
	System.out.println(rectangle.getArea());
	 
    }
}
