import java.util.Scanner;
import java.util.regex.*;
public class Program {
    public static void main(String args[]) {
	Scanner sc = new Scanner(System.in);
	String number = sc.nextLine();
	if (!isCorrect(number))
	    return;	
    }
    String getNextNum(String number, int lenNum) {
	
    }
    boolean isCorrect(String number) {
	String regex = "^\\d{2}[A-Z]{2}\\d{2}$";
	Pattern pattern = Pattern.compile(regex);
	Pattern sub_pattern = Pattern.compile("\\d{2}");
	Matcher matcher = pattern.matcher(number);
	Matcher sub_matcher = sub_pattern.matcher(number);
	if (!matcher.matches()) {
	    System.out.println("Неверный формат номера");
	    return false;
	}
	while(sub_matcher.find()) {
	    String group = sub_matcher.group().trim();
	    String except_number = "00";
	    System.out.println(group);
	    if (group.equals(except_number)) {
		System.out.println(except_number + " недопустимо");
		return false;
	    }
	}
	return true;
    }
}    
