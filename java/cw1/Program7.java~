interface Figures {
    void setCoor(int x, int y);
    boolean isVisible();
    void printParams();
}
class Figure implements Figures {
    int x = 0;
    int y = 0;
    boolean visible = true;
    void setCoor(int x, int y) {
	this.x = x;
	this.y = y;
    }
    

}
